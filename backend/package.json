{
  "name": "sales-tax-tracker",
  "version": "1.0.0",
  "description": "Comprehensive sales tax tracking application with POS and accounting integrations",
  "main": "dist/server.js",
  "scripts": {
    "start": "node -r tsconfig-paths/register dist/server.js",
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/server.ts",
    "build": "tsc",
    "debug": "nodemon --exec \"node --inspect -r ts-node/register -r tsconfig-paths/register src/server.ts\"",
    "dev:context7": "npm run context7:server &",
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:landing:headed": "playwright test tests/e2e/landing-page-styling.spec.ts --headed --project=chromium",
    "test:landing:debug": "playwright test tests/e2e/landing-page-styling.spec.ts --debug --project=chromium",
    "test:ui": "playwright test --ui",
    "test:watch": "jest --watch",
    "test:all": "npm run test && npm run test:e2e",
    "lint": "eslint src/ tests/",
    "lint:fix": "eslint src/ tests/ --fix",
    "typecheck": "tsc --noEmit",
    "context7:server": "npx @upstash/context7-mcp --transport http --port 3002",
    "db:migrate": "node scripts/migrate.js",
    "db:seed": "node scripts/seed.js",
    "tax-rates:update": "node scripts/update-tax-rates.js"
  },
  "keywords": [
    "sales-tax",
    "pos-integration",
    "accounting",
    "tax-compliance"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@auth0/auth0-react": "^2.4.0",
    "@clerk/backend": "^2.9.0",
    "@mendable/firecrawl-js": "^1.29.3",
    "@prisma/client": "^6.14.0",
    "@sentry/integrations": "^7.114.0",
    "@sentry/node": "^10.3.0",
    "@sentry/tracing": "^7.120.4",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/bull": "^3.15.9",
    "@types/ioredis": "^4.28.10",
    "@types/nodemailer": "^6.4.17",
    "@types/pdfkit": "^0.12.12",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@types/react-router-dom": "^5.3.3",
    "@types/redis": "^4.0.10",
    "@vitejs/plugin-react": "^5.0.0",
    "auth0": "^4.28.0",
    "autoprefixer": "^10.4.21",
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.16.5",
    "canvas": "^3.1.2",
    "chart.js": "^4.5.0",
    "chartjs-node-canvas": "^5.0.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-jwt": "^8.5.1",
    "express-rate-limit": "^6.10.0",
    "helmet": "^7.0.0",
    "ioredis": "^5.7.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "lucide-react": "^0.539.0",
    "mongoose": "^7.5.0",
    "multer": "^1.4.5-lts.1",
