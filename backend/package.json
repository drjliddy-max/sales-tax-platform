    "nodemailer": "^7.0.5",
    "pdfkit": "^0.14.0",
    "pg": "^8.16.3",
    "postcss": "^8.5.6",
    "prisma": "^6.14.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.0",
    "recharts": "^3.1.2",
    "redis": "^5.8.0",
    "tailwindcss": "^4.1.11",
    "vite": "^7.1.1",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-jwt": "^6.0.4",
    "@types/jest": "^29.5.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.5.0",
