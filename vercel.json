{
  "version": 2,
  "buildCommand": "cd Tracker/frontend && npm ci && npm run build",
  "outputDirectory": "Tracker/frontend/dist",
  "installCommand": "cd Tracker/frontend && npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}